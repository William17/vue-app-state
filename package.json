    "babel-jest": "^23.0.1",
    "jest": "^23.0.0",
    "jest-localstorage-mock": "^2.2.0",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-uglify": "^4.0.0",
